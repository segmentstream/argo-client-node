{
  "name": "@segmentstream/argo-client-node",
  "version": "0.4.1",
  "description": "Argo client for node generated from openapi spec",
  "repository": {
    "url": "git@github.com:segmentstream/argo-client-node.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
